<h2 style="color:crimson;">Productos</h2>
{{#if productos}}
<div class="table-responsive">
    <table class="table table-dark">
        <tr style="color: yellow;">
            <th>Title</th>
            <th>Price</th>
            <th>Thumbnail</th>
        </tr>
        {{#each productos}}
        <tr>
            <td>
                {{this.title}}
            </td>
            <td>
                {{this.price}}
            </td>
            <td>
                <img src="{{this.thumbnail}}" alt="{{this.title}}" width="50px" height="50px">
            </td>
        </tr>
        {{~/each}}
    </table>
</div>
{{else}}
<h3 class="alert alert-danger">No se encontraron datos</h1>
{{/if}} 
